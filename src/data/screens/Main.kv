<Main>:
    name: "Main"
    sm: sm

    MDBoxLayout:
        orientation: 'vertical'

        MDTopAppBar:
            right_action_items: [["magnify"], ["plus-circle-outline"]]
            title: f"[font={Mustica}]Musics[/font]"
            anchor_title: "left"
    
        MyMDTabs:
            text_color_normal: 0, 0, 0, .6
            tab_indicator_anim: True
            tab_hint_x: True

            MyTab:
                title: f"[font={Mustica}]Download[/font]"
                icon: "download"
                
                MDScreenManager:
                    info: info
                    id: sm

                    SearchVideos:

                    Info:
                        id: info
                        
                    Tools:

            MyTab:
                title: f"[font={Mustica}]Media Player[/font]"
                icon: "music"

                MyLabel:
                    text: "Media Player"
                    pos_hint: {"center_y":.5}
